
#define macro SINGLE_BIT_MASK_WRAPPER() = {
    0x04 calldataload
    SINGLE_BIT_MASK()
    0x00 mstore
    0x20 0x00 return
}

#define macro TOGGLE_BIT_WRAPPER() = {
    0x04 calldataload
    0x24 calldataload
    TOGGLE_BIT()
    0x00 mstore
    0x20 0x00 return
}



#define macro MAIN() = {
    0x00 calldataload 0xE0 shr
    dup1 __FUNC_SIG(singleBitMask) eq singleBitMask jumpi
    dup1 __FUNC_SIG(toggleBit) eq toggleBit jumpi


    0x00 0x00 revert

    singleBitMask:
        SINGLE_BIT_MASK_WRAPPER()

    toggleBit:
        TOGGLE_BIT_WRAPPER()

}